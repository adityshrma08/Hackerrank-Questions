#include <bits/stdc++.h>
using namespace std;

void max_of_four(int a , int b , int c , int d){
    if(a>b && a>c && a>d){
        cout << a << endl;
    }
    else
    {
        if(b>c && b>d){
            cout << b << endl;;
        }
        else{
            if(c>d){
                cout << c << endl;;
            }
            else {
            cout << d << endl;;
            }
        }
    }
}
void max_of_four(int,int,int,int);
int main() {
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    max_of_four(a, b, c, d);
    
    return 0;
}
